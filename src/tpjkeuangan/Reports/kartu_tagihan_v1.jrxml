	<queryString><![CDATA[SELECT nota.no_nota, coalesce(nama_kapal,'') as nama_kapal, to_char(tj.tgl_berangkat, 'dd-MM-yy') as tgl_berangkat, sum(coalesce(sub_total,0)) as total,
b.no_bayar, tgl_bayar, alat_pembayaran, jml_bayar, jml_klem, memo
from nota
inner join nota_detail d on nota.no_nota=d.no_nota
left join nota_kapal_tujuan tj on tj.serial_kode=nota.seri_kapal
left join kapal on kapal.kode_kapal=tj.kode_kapal
left join(
select dbyr.no_bayar, dbyr.no_nota, to_char(tanggal, 'dd-MM-yyyy') as tgl_bayar, coalesce(alat_pembayaran,'') as alat_pembayaran, 
coalesce(jumlah,0) as jml_bayar, coalesce(klem, 0) as jml_klem, coalesce(memo,'') as memo
from nota_pembayaran byr 
inner join nota_pembayaran_detail dbyr on byr.no_bayar=dbyr.no_bayar
left join nota_alat_pembayaran ap on ap.kode=alat_bayar 
) b on b.no_nota=nota.no_nota
group by nota.no_nota, coalesce(nama_kapal,'') , to_char(tj.tgl_berangkat, 'dd-MM-yy'),
b.no_bayar, tgl_bayar, alat_pembayaran, jml_bayar, jml_klem, memo]]></queryString>

