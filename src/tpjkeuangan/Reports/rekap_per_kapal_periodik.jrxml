	<parameter name="kota_tujuan" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["MERAUKE"]]></defaultValueExpression>
	</parameter>
	<parameter name="tanggal1" isForPrompting="true" class="java.lang.String"/>
	<parameter name="tanggal2" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select coalesce(nama_kota,'') as nama_kota, kondisi, coalesce(nama_kapal,'') as kapal, 
tgl_berangkat ,
sum(grand_total) as total_nota, sum(jml_bayar) as jml_bayar, sum(jml_klem) as jml_klem,
sum(grand_total) - sum(jml_bayar) +sum(jml_klem) as sisa
from(
	select d.no_nota, --case when toko.nama is null then coalesce(c.nama,'') else coalesce(toko.nama, '') end as nama_toko,
	sum(coalesce(sub_total, 0)) as grand_total, 0 as jml_bayar, 0 as jml_klem
	from nota_detail d inner join nota n on n.no_nota=d.no_nota
	inner join nota_kapal_tujuan k on k.serial_kode=n.seri_kapal
	where to_Char(tgl_berangkat,'yyyy-MM-dd')>=$P{tanggal1}
	and to_Char(tgl_berangkat,'yyyy-MM-dd')<=$P{tanggal2}
	group by d.no_nota

	union all
	select d.no_nota, 0 as total_nota, sum(coalesce(jumlah,0)) as jml_bayar, sum(coalesce(klem,0)) as jml_klem
	from nota_pembayaran_detail d
	inner join nota_pembayaran p on p.no_bayar=d.no_bayar 
	inner join nota on nota.no_nota=d.no_nota
	inner join nota_kapal_tujuan k on k.serial_kode=nota.seri_kapal
	where  to_Char(tgl_berangkat,'yyyy-MM-dd')>=$P{tanggal1}
	and to_Char(tgl_berangkat,'yyyy-MM-dd')<=$P{tanggal2}
	group by d.no_nota
)a
inner join nota on nota.no_nota=a.no_nota 
inner join nota_kapal_tujuan ktj on ktj.serial_kode=nota.seri_kapal
inner join kapal on kapal.kode_kapal=ktj.kode_kapal
inner join kota on kota.kode_kota=ktj.kota_tujuan
and to_Char(tgl_berangkat,'yyyy-MM-dd')>=$P{tanggal1}
and to_Char(tgl_berangkat,'yyyy-MM-dd')<=$P{tanggal2}
group by coalesce(nama_kota,''), kondisi, kondisi, coalesce(nama_kapal,'') , 
tgl_berangkat order by coalesce(nama_kota,''), tgl_berangkat, coalesce(nama_kapal,'')]]></queryString>

