	<queryString><![CDATA[select p.no_bayar, tanggal, case when c.nama is not null then c.nama else coalesce(tk.nama,'') end as customer, 
coalesce(p.alat_bayar,'') as kode_alat_bayar, coalesce(alat_pembayaran,'') as alat_pembayaran, coalesce(check_no, '') as no_cek, 
case when trim(coalesce(check_no,''))='' then '' else to_char(tgl_cek,'dd-MM-yyyy') end as tgl_cek,
no_nota, jumlah, diskon, denda, klem, (jumlah-diskon+klem) as sub_total, p.user_tr
from nota_pembayaran p
inner join nota_pembayaran_detail d on d.no_bayar=p.no_bayar
left join customer c on c.kode_cust=p.kode_cust and to_tujuan=true
left join toko tk on tk.kode_toko=p.kode_cust and to_tujuan=false 
left join nota_alat_pembayaran ap on ap.kode=p.alat_bayar
where to_Char(tanggal, 'yyyy-MM-dd')>=$P{tanggal1}
and to_Char(tanggal, 'yyyy-MM-dd')<=$P{tanggal2}
and p.alat_bayar like $P{jenis_pembayaran}||'%'
order by coalesce(p.alat_bayar,''), p.tanggal, case when c.nama is not null then c.nama else coalesce(tk.nama,'') end]]></queryString>

