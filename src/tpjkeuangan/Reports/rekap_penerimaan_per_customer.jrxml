	<parameter name="to_tujuan" isForPrompting="false" class="java.lang.Boolean"/>
	<parameter name="customer" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select coalesce(alat_pembayaran,'') as alat_pembayaran, case when to_tujuan is true then c.nama else toko.nama end , 
sum(coalesce(jumlah,0)) as jml_bayar, sum(coalesce(klem,0)) as jml_klem, sum(coalesce(jumlah,0)+coalesce(klem,0)) as sub_total
from nota_pembayaran n 
inner join nota_pembayaran_detail d on  n.no_bayar=d.no_bayar
left join nota_alat_pembayaran ab on n.alat_bayar=ab.kode 
left join customer c on c.kode_cust=n.kode_cust 
left join toko on toko.kode_toko=n.kode_cust 
where to_Char(tanggal, 'yyyy-MM-dd') >= $P{tanggal1}
and to_Char(tanggal, 'yyyy-MM-dd') <= $P{tanggal2}
and to_tujuan=$P{to_tujuan}
and n.kode_cust like $P{customer}||'%'
and void_check=false
group by ab.priority, coalesce(alat_pembayaran,'') , case when to_tujuan is true then c.nama else toko.nama end
order by ab.priority, case when to_tujuan is true then c.nama else toko.nama end
--select * from nota_pembayaran_detail]]></queryString>

