	<parameter name="kota_tujuan" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["MERAUKE"]]></defaultValueExpression>
	</parameter>
	<parameter name="kapal_berangkat" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA["KAPAL MULIANIM Berangkat Tgl, 12-09-2008"]]></defaultValueExpression>
	</parameter>
	<parameter name="serial_kapal" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select kondisi, case when tagihan_per='T' then c.nama else toko.nama end,
sum(grand_total) as total_nota, sum(jml_bayar) as jml_bayar, sum(jml_klem) as jml_klem,
sum(grand_total- jml_bayar + jml_klem) as sisa
from(
	select d.no_nota, --case when toko.nama is null then coalesce(c.nama,'') else coalesce(toko.nama, '') end as nama_toko,
	sum(coalesce(sub_total, 0)) as grand_total, 0 as jml_bayar, 0 as jml_klem
	from nota_detail d inner join nota n on n.no_nota=d.no_nota
	where seri_kapal=$P{serial_kapal} and is_batal=false
	and (coalesce(nota_header,'')='' or is_header=true)
	group by d.no_nota
	union all
	select d.no_nota, --case when toko.nama is null then coalesce(c.nama,'') else coalesce(toko.nama, '') end as nama_toko,
	sum(coalesce(sub_total, 0)) as grand_total, 0 as jml_bayar, 0 as jml_klem
	from nota_detail d 
	inner join nota n on n.no_nota=d.no_nota
	inner join nota nh on nh.no_nota=n.nota_header
	where nh.seri_kapal=$P{serial_kapal} and nh.is_batal=false
	and (coalesce(n.nota_header,'')<>'')
	group by d.no_nota

	union all

	select d.no_nota, 0 as total_nota, sum(coalesce(jumlah,0)) as jml_bayar, sum(coalesce(klem,0)) as jml_klem
	from nota_pembayaran_detail d
	inner join nota_pembayaran p on p.no_bayar=d.no_bayar
	inner join nota on nota.no_nota=d.no_nota
	where  d.is_batal=false
	and seri_kapal=$P{serial_kapal}
	group by d.no_nota

)a
inner join nota on nota.no_nota=a.no_nota
left join toko on toko.kode_toko=nota.customer
and (case when tagihan_per='P' then toko.kode_toko=customer else toko.kode_toko='!!!!!!!!!' end)
left join customer c on c.kode_cust=nota.customer 
and (case when tagihan_per='T' then c.kode_cust=customer else c.kode_cust='!!!!!!!!!' end)
group by kondisi, case when tagihan_per='T' then c.nama else toko.nama end]]></queryString>

